<h4><%= @vitamin.contents %></h4>
<h6><a href="/vitamins/<%= @vitamin.id %>/edit">Edit</a></h6>
<h5>Notes:</h5>
<% @vitamin.notes.each do |note| %>
  <p><%= note.content %></p>
  <h6>-<%= note.user.username %></h6>
  <h6><a href="/notes/<%= note.id %>/edit">Edit Note</a></h6>
  <form class="" action="index.html" method="post">
    <input type="hidden" id="hidden" name="_method" value="delete">
    <input type="submit" name="" value="Delete Note">
  </form>
<% end %>

<a href="/vitamins/<%= @vitamin.id %>/notes/new">New Note</a>
<form class="" action="/vitamins/<%= @vitamin.id %>/delete" method="post">
  <input type="hidden" id="hidden" name="_method" value="delete">
  <input type="submit" name="" value="Delete Vitamin">
</form>
